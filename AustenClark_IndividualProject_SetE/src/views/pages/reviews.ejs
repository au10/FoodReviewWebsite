<%- include('../partials/header'); -%>
<%- include('../partials/menu') %>
<body>
  <main>
      <div class="container-fluid" style="padding: 2%; ">
        <form class="" action="/get_feed/review/filter" method="post">
          <div class="row">
            <div class="col-3">

            </div>
            <div class="col-5">
              <input class="form-control" type="text" name="name" placeholder="Enter Food Name">
            </div>
            <div class="col-1">
              <input class="btn btn-light" type="submit" name="Submit" value = "Submit"/>
            </div>
          </div>
        </form>
      </div>
    <div class="container-lg" style="margin-top: 2%;">
      <% if (error) { %>
        <div class="alert alert-danger"><%- message %></div>
      <% } %>
      <%- include('../partials/reviews', {items: items}); %>
    </div>
  </main>
<%- include('../partials/footer'); -%>